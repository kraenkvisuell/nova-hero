{
    "name": "kraenkvisuell/nova-hero",
    "description": "Content Block Package for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "content blocks"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "classic-o/nova-media-library": "^1.0",
        "manogi/nova-tiptap": "^1.0",
        "optimistdigital/nova-translatable": "^1.5",
        "spatie/laravel-translatable": "^4.4",
        "whitecube/nova-flexible-content": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "Kraenkvisuell\\NovaHero\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kraenkvisuell\\NovaHero\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "~3.0",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraenkvisuell\\NovaHero\\NovaHeroServiceProvider"
            ],
            "aliases": [
                "Kraenkvisuell\\NovaHero\\Facades\\Hero"
            ]
        }
    }
}
